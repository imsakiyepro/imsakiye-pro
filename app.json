{
  "expo": {
    "name": "İmsakiye Pro",
    "slug": "imsakiye-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-optimized.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-optimized.png",
      "resizeMode": "contain",
      "backgroundColor": "#05140A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/logo-optimized.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.tunahankzlbg.imsakiye",
      "buildNumber": "7",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Bulunduğunuz konuma özel namaz vakitlerini hesaplamak, Kıble yönünü belirlemek ve size en yakın camileri göstermek için konum izni gereklidir.",
        "UIBackgroundModes": [
          "remote-notification",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.tunahankzlbg.imsakiye",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-optimized.png",
        "backgroundColor": "#05140A"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS",
        "VIBRATE",
        "WAKE_LOCK",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/logo-optimized.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "2.0.21",
            "extraGradleProps": [
              "org.gradle.jvmargs=-Xmx3g -XX:MaxMetaspaceSize=512m"
            ]
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Bulunduğunuz konuma özel namaz vakitlerini hesaplamak, Kıble yönünü belirlemek ve size en yakın camileri göstermek için konum izni gereklidir."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#D4AF37",
          "defaultChannel": "default"
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "db9d5209-d612-4da2-9821-622b19c17b84"
      },
      "router": {}
    },
    "owner": "tunahankzlbg4"
  }
}